{
  "name": "kam-admin",
  "version": "0.0.1",
  "description": "Kamailio 管理后台（Spring Boot 后端）",
  "private": true,
  "scripts": {
    "backend:build": "mvn clean package -pl kam-admin-server -am -DskipTests",
    "backend:run": "mvn spring-boot:run -pl kam-admin-server",
    "docker:build": "docker build -f kam-admin-server/Dockerfile -t kam-admin-server:0.0.1 .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  }
}
